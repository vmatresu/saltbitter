compliance_architecture:
 gdpr:
  data_protection_officer: Required role for psychological data processing
  consent_management: Explicit opt-in for attachment assessment (Article 9)
  data_export: Automated export API within 30 days
  data_deletion: 30-day retention policy after account deletion
  breach_notification: 72-hour automated alerting to supervisory authority

 eu_ai_act:
  ai_labeling: ðŸ¤– badge on all AI profiles (Article 52)
  transparency_logs: All AI interactions logged with timestamps
  opt_out_mechanism: User settings for AI features (pre and post signup)
  audit_trail: Quarterly compliance reports + automated testing

 california_sb_243:
  disclosure_onboarding: AI disclosure shown during signup
  opt_out_before_signup: Pre-registration opt-out available
  human_alternatives: All AI features optional (practice with real users)
  quarterly_audits: Compliance verification + public transparency reports

