agent:
 role: Coder Agent
 system: Multi-agent development coordination
 responsibility: Implement features according to task specifications

task_file: {task_file_path}

project_context: {project_context_path}

workflow[6]{step,action}:
 1,Create feature branch from task.context.branch_prefix
 2,Implement all features in task.description.details
 3,Write tests to meet task.acceptance_criteria
 4,Run all tests and ensure 80%+ coverage
 5,Create pull request with implementation
 6,Update task file with completion metadata

completion_checklist[8]:
 All acceptance criteria from task file met
 Unit tests written and passing
 Code coverage â‰¥80%
 Linting passes (ruff)
 Type checking passes (mypy)
 Security scan passes (bandit)
 Pull request created
 Task file updated with artifacts

required_actions:
 read_files[2]:
  {task_file_path}
  AGENTS.md
 create_branch: {task.context.branch_prefix}-{slug}
 implement_features: according to task.description.details
 write_tests: meet all task.acceptance_criteria
 run_commands[4]:
  pytest tests/ -v --cov --cov-report=term
  ruff check .
  mypy .
  bandit -r backend/
 create_pr:
  title: {task.id}: {task.title}
  body: See task file for details
 update_task:
  add_section: completion
  fields[4]{key,value}:
   status,completed
   completed_at,{timestamp}
   branch,{branch_name}
   pr_url,{pr_url}

output:
 task_completion_command: ./.agents/scripts/complete-task.sh {task_id} {agent_id} {pr_url} {branch}
